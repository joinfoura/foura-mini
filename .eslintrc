{
  "env": {
    "es6": true,
    "node": true,
    "jest": true
  },
  "extends": [
    "airbnb-base"
  ],
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "operator-linebreak": [
      2,
      "after"
    ],
    "no-console": "off",
    "camelcase": "off",
    "comma-dangle": [
      "error",
      "never"
    ],
    "no-warning-comments": [
      2,
      {
        "terms": [
          "todo",
          "fixme"
        ],
        "location": "anywhere"
      }
    ],
    "max-len": [
      "error",
      {
        "code": 150
      }
    ],
    "implicit-arrow-linebreak": [
      "warn",
      "beside"
    ],
    "function-paren-newline": [
      "warn"
    ],
    "no-unused-vars": [
      "error",
      {
        "varsIgnorePattern": "^_"
      }
    ]
  }
}
